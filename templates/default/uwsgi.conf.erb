<uwsgi>
  <% if @options["socket"] %>
  <socket><%= @options["socket"] %></socket>
  <% else %>
  <socket><%= @options["host"]%>:<%= @options["port"] %></socket>
  <% end %>

  <pythonpath><%= @options["home"] %></pythonpath>

  <% if @options["processes"] %>
  <processes><%= @options["processes"] %></processes>
  <% end %>

  <% if @options["master"] %>
  <master/>
  <% end %>

  <home><%= @options["env"] %></home>
  <venv><%= @options["env"] %></venv>

  <wsgi-file><%= @options["wsgi"] %></wsgi-file>

  <% if @options["user"] %>
  <gid><%= @options["user"] %></gid>
  <% end %>

  <% if @options["group"] %>
  <uid><%= @options["group"] %></uid>
  <% end %>

  <% if @options["log-master"] %>
  <log-master />
  <daemonize />
  <% else %>
  <daemonize><%= @options["logfile"] %></daemonize>
  <logto><%= @options["logfile"] %></logto>
  <% end %>

  <% if @options["pidfile"] %>
  <pidfile><%= @options["pidfile"] %></pidfile>
  <% end %>

  <% if @options["no-orphas"] %>
  <no-orphans />
  <% end %>

  <% if @options["workers"] %>
  <workers><%= @options["workers"] %></workers>
  <% end %>

  <% if @options["reload-mercy"] %>
  <reload-mercy><%= @options["reload-mercy"] %></reload-mercy>
  <% end %>

  <% if @options["chmod-socket"] %>
  <chmod-socket><%= @options["chmod-socket"] %></chmod-socket>
  <% end %>

  <% if @options["harakiri"] %>
  <harakiri><%= @options["harakiri"] %></harakiri>
  <% end %>

  <% if @options["log-slow"] %><log-slow><% @options["log-slow"] %></log-slow><% end %>

  <% if @options["log-5xx"] %><log-5xx /><% end %>

  <% if @options["log-4xx"] %><log-4xx /><% end %>

  <% if @options["memory-report"] %><memory-report /><% end %>

  <% if @options["reload-on-as"] %><reload-on-as><%= @options["reload-on-as"] %></reload-on-as><% end %>
</uwsgi>
